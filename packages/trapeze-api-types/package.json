{
  "name": "@donmahallem/trapeze-api-types",
  "version": "5.0.7",
  "description": "Api Types and helper for TrapezeApi",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "keywords": [
    "api",
    "types"
  ],
  "scripts": {
    "build": "tsc",
    "test": "mocha --config ../../.mocharc.yml",
    "test:coverage": "nyc --nycrc-path ../../.nycrc.json npm run test",
    "lint": "tslint -c tslint.json -p tsconfig.json src/**/*.ts",
    "lint:fix": "tslint --fix -c tslint.json -p tsconfig.json src/**/*.ts",
    "docs": "typedoc --options ../../typedoc.json"
  },
  "homepage": "https://donmahallem.github.io/trapeze/",
  "author": {
    "name": "donmahallem",
    "email": "donmahallem@users.noreply.github.com",
    "url": "https://github.com/donmahallem"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/donmahallem/trapeze/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/donmahallem/trapeze.git"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "dist/*",
    "src/*",
    "!src/**/*.spec.ts"
  ],
  "publishConfig": {
    "access": "public"
  }
}
