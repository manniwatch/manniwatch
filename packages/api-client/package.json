{
    "name": "@manniwatch/trapeze-api-client",
    "version": "5.0.7",
    "description": "Node Client to consume the Trapeze Api",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "private": false,
    "scripts": {
        "build": "tsc",
        "test": "mocha --config ../../.mocharc.yml",
        "test:coverage": "nyc --nycrc-path ../../.nycrc.json npm run test",
        "lint": "tslint -c tslint.json -p tsconfig.json src/**/*.ts",
        "lint:fix": "tslint --fix -c tslint.json -p tsconfig.json src/**/*.ts",
        "docs": "typedoc --options ../../typedoc.json",
        "coverage:upload": "codecov --flags trapeze-api-client"
    },
    "dependencies": {
        "@manniwatch/trapeze-api-types": "^5.0.7",
        "request": "~2.88.2",
        "request-promise-native": "~1.0.8"
    },
    "devDependencies": {
        "@types/request": "~2.48.4",
        "@types/request-promise-native": "~1.0.17",
        "nock": "~12.0.1"
    },
    "files": [
        "dist/*",
        "src/*"
    ],
    "homepage": "https://manniwatch.github.io/trapeze/",
    "author": {
        "name": "manniwatch",
        "email": "manniwatch@users.noreply.github.com",
        "url": "https://github.com/manniwatch"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/manniwatch/trapeze/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/manniwatch/trapeze.git"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}