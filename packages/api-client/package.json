{
    "name": "@manniwatch/api-client",
    "version": "0.13.2",
    "description": "Node Client to consume the Api",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/types/index.d.ts",
    "private": false,
    "scripts": {
        "build": "npm run build:esm && npm run build:cjs && npm run build:types && npm run build:version",
        "build:esm": "tsc --project ./tsconfig.json --module es2015 --outDir dist/esm",
        "build:cjs": "tsc --project ./tsconfig.json --module commonjs --outDir dist/cjs",
        "build:types": "tsc --project ./tsconfig.json  -d --declarationDir dist/types --declarationMap --emitDeclarationOnly",
        "build:version": "node ./scripts/replace_build_version.js",
        "version": "npm run build:version",
        "test": "mocha --config ../../.mocharc.yml",
        "test:coverage": "nyc --nycrc-path ../../.nycrc.json npm run test",
        "lint": "tslint -c tslint.json -p tsconfig.json src/**/*.ts",
        "lint:fix": "tslint --fix -c tslint.json -p tsconfig.json src/**/*.ts",
        "docs": "typedoc --options ../../typedoc.json",
        "prepublishOnly": "npm run test && npm run docs && npm run build"
    },
    "dependencies": {
        "@manniwatch/api-types": "^0.13.2",
        "request": "~2.88.2",
        "request-promise-native": "~1.0.8"
    },
    "devDependencies": {
        "@types/request": "~2.48.5",
        "@types/request-promise-native": "~1.0.17",
        "glob": "~7.1.6",
        "nock": "~13.0.2",
        "tslint": "~6.1.2"
    },
    "files": [
        "dist/*",
        "src/*"
    ],
    "homepage": "https://manniwatch.github.io/manniwatch/",
    "author": {
        "name": "manniwatch",
        "email": "manniwatch@users.noreply.github.com",
        "url": "https://github.com/manniwatch"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/manniwatch/manniwatch/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/manniwatch/manniwatch.git"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
