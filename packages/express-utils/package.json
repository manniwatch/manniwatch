{
  "name": "@manniwatch/express-utils",
  "version": "0.19.1",
  "description": "Helpers for express",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "keywords": [
    "api",
    "types"
  ],
  "scripts": {
    "build": "tsc",
    "test": "mocha --config ../../.mocharc.yml",
    "test:coverage": "nyc --nycrc-path ../../.nycrc.json npm run test",
    "lint": "tslint -c tslint.json -p tsconfig.json src/**/*.ts",
    "lint:fix": "tslint --fix -c tslint.json -p tsconfig.json src/**/*.ts",
    "docs": "typedoc --options ../../typedoc.json",
    "prepublishOnly": "npm run test && npm run docs && npm run build"
  },
  "homepage": "https://manniwatch.github.io/manniwatch/",
  "author": {
    "name": "donmahallem",
    "email": "donmahallem@users.noreply.github.com",
    "url": "https://github.com/donmahallem"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/manniwatch/manniwatch/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manniwatch/manniwatch.git"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/express": "4.17.11",
    "apidoc": "0.26.0",
    "axios": "0.21.1",
    "express": "4.17.1",
    "jsonschema": "1.4.0",
    "supertest": "6.1.2"
  },
  "peerDependencies": {
    "express": "~4.17.1",
    "jsonschema": "~1.4.0"
  }
}
