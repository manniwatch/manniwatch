{
  "name": "@manniwatch/api-proxy-router",
  "version": "0.29.15",
  "description": "An express api router used to proxy requests to the upstream server",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "private": false,
  "keywords": [
    "api",
    "types"
  ],
  "scripts": {
    "build": "rollup -c ./rollup.config.js && npm run build:types",
    "build:types": "tsc --project ./tsconfig.json  -d --declarationDir dist/types --declarationMap --emitDeclarationOnly",
    "build:readme": "npx @appnest/readme generate --input ./readme_blueprint.md --config readme_config.json",
    "test": "mocha --config ../../.mocharc.yml --timeout 30000",
    "test:coverage": "nyc --nycrc-path ../../.nycrc.json npm run test",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
    "postlint": "prettier --config ./../../.prettierrc src/**/*.ts --check",
    "lint:fix": "eslint -c .eslintrc.js --ext .ts ./src --fix",
    "postlint:fix": "prettier --config ./../../.prettierrc src/**/*.ts --write",
    "docs": "typedoc --options ./typedoc.config.js",
    "apidocs": "apidoc --line-ending LF README.md -i ./src/ -o ./docs/apidoc",
    "postdocs": "npm run apidocs",
    "prepublishOnly": "npm run test && npm run docs && npm run build",
    "version": "npm run build:readme && git add README.md",
    "postclean": "npx rimraf ./dist ./docs ./coverage"
  },
  "exports": {
    "require": "./dist/cjs/index.cjs",
    "import": "./dist/esm/index.mjs"
  },
  "homepage": "https://manniwatch.github.io/docs/api-proxy-router/index.html",
  "author": {
    "name": "donmahallem",
    "email": "donmahallem@users.noreply.github.com",
    "url": "https://github.com/donmahallem"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/manniwatch/manniwatch/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manniwatch/manniwatch.git"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@donmahallem/turbo": "~2.4.1",
    "@donmahallem/turbo-validate-request": "~0.4.1",
    "@manniwatch/api-client": "^0.29.15",
    "@manniwatch/api-types": "^0.29.15",
    "@manniwatch/schemas": "^0.29.15",
    "ajv": "~8.11.0",
    "async-mutex": "~0.3.2",
    "node-cache": "~5.1.2"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/helmet": "4.0.0",
    "@types/proxyquire": "1.3.28",
    "@types/supertest": "2.0.12",
    "apidoc": "0.51.1",
    "axios": "0.27.2",
    "express": "4.18.1",
    "proxyquire": "2.1.3",
    "supertest": "6.2.3"
  },
  "peerDependencies": {
    "express": "~4.18.0"
  }
}
