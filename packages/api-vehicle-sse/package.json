{
  "name": "@manniwatch/api-vehicle-sse",
  "version": "0.4.0",
  "description": "An express router handling server sent events for vehicle updates",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "keywords": [
    "api",
    "types"
  ],
  "scripts": {
    "build": "tsc",
    "test": "mocha --config ../../.mocharc.yml",
    "test:coverage": "nyc --nycrc-path ../../.nycrc.json npm run test",
    "lint": "tslint -c tslint.json -p tsconfig.json src/**/*.ts",
    "lint:fix": "tslint --fix -c tslint.json -p tsconfig.json src/**/*.ts",
    "docs": "typedoc --options ../../typedoc.json",
    "prepublishOnly": "npm run test && npm run docs && npm run build"
  },
  "homepage": "https://manniwatch.github.io/manniwatch/",
  "author": {
    "name": "manniwatch",
    "email": "manniwatch@users.noreply.github.com",
    "url": "https://github.com/manniwatch"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/manniwatch/manniwatch/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manniwatch/manniwatch.git"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@manniwatch/api-client": "^0.4.0",
    "@manniwatch/api-types": "^0.4.0",
    "@manniwatch/express-utils": "^0.4.0",
    "@manniwatch/vehicle-location-diff": "^0.4.0",
    "express": "~4.17.1",
    "jsonschema": "~1.2.5",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@types/express": "~4.17.3",
    "@types/helmet": "~0.0.45",
    "@types/request-promise-native": "^1.0.17",
    "@types/supertest": "~2.0.8",
    "apidoc": "~0.20.1",
    "request-promise-native": "^1.0.8",
    "supertest": "~4.0.2",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  },
  "peerDependencies": {
    "request-promise-native": "^1.0.8"
  }
}
