{
  "name": "@manniwatch/pb-converter",
  "version": "0.6.0",
  "description": "Api Types and helper for Trapeze Api",
  "main": "dist/commonjs/index.js",
  "types": "dist/commonjs/index.d.ts",
  "private": false,
  "keywords": [
    "api",
    "types"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:commonjs",
    "build:esm": "tsc --module ES2015 --outDir ./dist/esm",
    "build:commonjs": "tsc --outDir ./dist/commonjs",
    "test": "mocha --config ../../.mocharc.yml",
    "test:coverage": "nyc --nycrc-path ../../.nycrc.json npm run test",
    "lint": "tslint -c tslint.json -p tsconfig.json src/**/*.ts",
    "lint:fix": "tslint --fix -c tslint.json -p tsconfig.json src/**/*.ts",
    "docs": "typedoc --options ../../typedoc.json",
    "prepublishOnly": "npm run test && npm run docs && npm run build"
  },
  "module": "./dist/esm/entry.js",
  "homepage": "https://manniwatch.github.io/manniwatch/",
  "author": {
    "name": "manniwatch",
    "email": "manniwatch@users.noreply.github.com",
    "url": "https://github.com/manniwatch"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/manniwatch/manniwatch/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manniwatch/manniwatch.git"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "dist/*",
    "src/*",
    "!src/**/*.spec.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "tslint": "~6.1.2",
    "typescript": "~3.8.3"
  },
  "dependencies": {
    "@manniwatch/api-types": "^0.6.0",
    "@manniwatch/pb-types": "^0.6.2"
  }
}
