{
  "name": "@manniwatch/client-desktop",
  "version": "0.18.0",
  "description": "An electron app to be used with manniwatch endpoints",
  "main": "./dist/main.js",
  "private": false,
  "bin": {
    "manniwatch": "./bin/client"
  },
  "scripts": {
    "build": "tsc",
    "build:readme": "npx @appnest/readme generate --input ../package_readme_blueprint.md --config readme_config.json --silent true",
    "test": "mocha --config ../../.mocharc.yml",
    "test:watch": "mocha --config ../../.mocharc.yml --watch",
    "test:coverage": "nyc --nycrc-path ../../.nycrc.json npm run test",
    "lint": "tslint -c tslint.json -p tsconfig.json src/**/*.ts",
    "lint:fix": "tslint --fix -c tslint.json -p tsconfig.json src/**/*.ts",
    "docs": "typedoc --options ../../typedoc.json",
    "prepublishOnly": "npm run test && npm run docs && npm run build"
  },
  "files": [
    "dist/*",
    "bin/*"
  ],
  "manniwatch": "./bin/manniwatch",
  "engines": {
    "node": ">=10.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://manniwatch.github.io/manniwatch/",
  "author": {
    "name": "donmahallem",
    "email": "donmahallem@users.noreply.github.com",
    "url": "https://github.com/donmahallem"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/manniwatch/manniwatch/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manniwatch/manniwatch.git"
  },
  "devDependencies": {
    "@types/chai": "~4.2.14",
    "@types/mocha": "~8.0.4",
    "@types/sinon": "~9.0.8",
    "asar": "~3.0.3",
    "chai": "~4.2.0",
    "mocha": "~8.2.1",
    "nyc": "~15.1.0",
    "pug-cli": "~1.0.0-alpha6",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "typedoc": "~0.19.2"
  },
  "dependencies": {
    "@manniwatch/api-proxy-router": "^0.18.0",
    "@manniwatch/api-types": "^0.18.0",
    "@manniwatch/client-ng": "^0.18.0",
    "electron": "~11.0.0",
    "express": "~4.17.1",
    "helmet": "~4.2.0",
    "yargs": "~16.1.1"
  }
}
