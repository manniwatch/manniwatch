{
    "name": "@manniwatch/client-ng",
    "description": "Angular Client for Manniwatch",
    "version": "0.29.15",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "prebuild": "npm run build:pug",
        "build": "ng build --configuration=olvector && ng build",
        "prebuild:build:prod": "npm run build:pug",
        "build:prod": "ng build --aot --configuration=production && ng build --aot --configuration=production,olvector",
        "build:pug": "pug ./src --doctype html --pretty",
        "build:readme": "npx @appnest/readme generate --input ../package_readme_blueprint.md --config readme_config.json",
        "build:release": "ng build --prod --aot",
        "test": "ng test --watch=false --browsers=ChromeHeadless",
        "test:coverage": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
        "test:watch": "ng test",
        "lint": "ng lint",
        "postlint": "prettier --config ./../../.prettierrc src/**/*.ts src/**/*.scss --check",
        "lint:fix": "ng lint --fix",
        "postlint:fix": "prettier --config ./../../.prettierrc src/**/*.ts src/**/*.scss --write",
        "e2e": "ng e2e",
        "postinstall": "npm run build:pug",
        "docs": "npx compodoc ./src --config ./.compodocrc.json",
        "prepare": "node ./scripts/prepare.js",
        "prepublishOnly": "npm run test && npm run docs && npm run build:prod",
        "build:stats": "ng build --stats-json",
        "analyze": "webpack-bundle-analyzer dist/manniwatch/stats.json",
        "version": "npm run build:readme && git add README.md"
    },
    "browser": {
        "fs": false
    },
    "private": false,
    "homepage": "https://github.com/manniwatch/manniwatch/tree/master/packages/client-types",
    "author": {
        "name": "donmahallem",
        "email": "donmahallem@users.noreply.github.com",
        "url": "https://github.com/donmahallem"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/manniwatch/manniwatch/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/manniwatch/manniwatch.git"
    },
    "engines": {
        "node": ">=14"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist/manniwatch/*",
        "src/*"
    ],
    "dependencies": {
        "@angular/animations": "13.3.0",
        "@angular/cdk": "13.3.1",
        "@angular/common": "13.3.0",
        "@angular/core": "13.3.0",
        "@angular/forms": "13.3.0",
        "@angular/localize": "13.3.0",
        "@angular/material": "13.3.1",
        "@angular/platform-browser": "13.3.0",
        "@angular/platform-browser-dynamic": "13.3.0",
        "@angular/router": "13.3.0",
        "@angular/service-worker": "13.3.0",
        "@donmahallem/rxjs-zone": "0.3.17",
        "@manniwatch/api-types": "^0.29.15",
        "@manniwatch/assets": "^0.29.15",
        "date-fns": "2.28.0",
        "deepmerge": "4.2.2",
        "karma": "6.3.17",
        "material-design-icons": "3.0.1",
        "ol": "6.9.0",
        "ol-mapbox-style": "7.1.1",
        "rxjs": "7.5.5",
        "webpack": "5.70.0",
        "zone.js": "0.11.5"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "13.3.0",
        "@angular-eslint/builder": "13.1.0",
        "@angular-eslint/eslint-plugin": "13.1.0",
        "@angular-eslint/eslint-plugin-template": "13.1.0",
        "@angular-eslint/schematics": "13.1.0",
        "@angular-eslint/template-parser": "13.1.0",
        "@angular/cli": "13.3.0",
        "@angular/compiler": "13.3.0",
        "@angular/compiler-cli": "13.3.0",
        "@angular/language-service": "13.3.0",
        "@compodoc/compodoc": "1.1.19",
        "@manniwatch/api-client": "^0.29.15",
        "@manniwatch/client-types": "^0.29.15",
        "@types/jasmine": "4.0.0",
        "@types/jasminewd2": "2.0.10",
        "@types/node": "16.11.26",
        "apply-loader": "2.0.0",
        "codelyzer": "6.0.2",
        "eslint": "8.11.0",
        "express": "4.17.3",
        "jasmine-core": "4.0.1",
        "jasmine-spec-reporter": "7.0.0",
        "karma": "6.3.17",
        "karma-chrome-launcher": "3.1.1",
        "karma-coverage": "2.2.0",
        "karma-coverage-istanbul-reporter": "3.0.3",
        "karma-jasmine": "4.0.1",
        "karma-jasmine-html-reporter": "1.7.0",
        "ng-packagr": "13.3.0",
        "protractor": "7.0.0",
        "pug": "3.0.2",
        "pug-cli": "1.0.0-alpha6",
        "tslib": "2.3.1",
        "typescript": "4.6.2",
        "webpack": "5.65.0",
        "webpack-bundle-analyzer": "4.5.0"
    }
}
