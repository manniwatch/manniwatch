{
    "name": "@manniwatch/client-ng",
    "description": "Angular Client for Manniwatch",
    "version": "0.30.1",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --configuration=olvector && ng build",
        "build:prod": "ng build --aot --configuration=production && ng build --aot --configuration=production,olvector",
        "build:readme": "npx @appnest/readme generate --input ../package_readme_blueprint.md --config readme_config.json",
        "build:release": "ng build --prod --aot",
        "test": "ng test --watch=false --browsers=ChromeHeadless",
        "test:coverage": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
        "test:watch": "ng test",
        "lint": "ng lint",
        "postlint": "prettier --config ./../../.prettierrc src/**/*.ts src/**/*.scss --check",
        "lint:fix": "ng lint --fix",
        "postlint:fix": "prettier --config ./../../.prettierrc src/**/*.ts src/**/*.scss --write",
        "e2e": "ng e2e",
        "docs": "npx compodoc ./src --config ./.compodocrc.json",
        "prepare": "node ./scripts/prepare.js",
        "prepublishOnly": "npm run test && npm run docs && npm run build:prod",
        "build:stats": "ng build --stats-json",
        "analyze": "webpack-bundle-analyzer dist/manniwatch/stats.json",
        "version": "npm run build:readme && git add README.md"
    },
    "browser": {
        "fs": false
    },
    "private": false,
    "homepage": "https://github.com/manniwatch/manniwatch/tree/master/packages/client-types",
    "author": {
        "name": "donmahallem",
        "email": "donmahallem@users.noreply.github.com",
        "url": "https://github.com/donmahallem"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/manniwatch/manniwatch/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/manniwatch/manniwatch.git"
    },
    "engines": {
        "node": ">=18"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist/manniwatch/*",
        "src/*"
    ],
    "dependencies": {
        "@angular/animations": "21.1.3",
        "@angular/cdk": "21.1.3",
        "@angular/common": "21.1.3",
        "@angular/core": "21.1.3",
        "@angular/forms": "21.1.3",
        "@angular/localize": "21.1.3",
        "@angular/material": "21.1.3",
        "@angular/platform-browser": "21.1.3",
        "@angular/platform-browser-dynamic": "21.1.3",
        "@angular/router": "21.1.3",
        "@angular/service-worker": "21.1.3",
        "@angular/ssr": "21.1.2",
        "@donmahallem/rxjs-zone": "0.6.38",
        "@manniwatch/api-types": "^0.30.0",
        "@manniwatch/assets": "^0.30.0",
        "date-fns": "4.1.0",
        "deepmerge": "4.3.1",
        "karma": "6.4.4",
        "material-design-icons": "3.0.1",
        "ol": "9.2.4",
        "ol-mapbox-style": "13.2.0",
        "rxjs": "7.8.2",
        "zone.js": "0.16.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "21.1.2",
        "@angular-eslint/builder": "21.2.0",
        "@angular-eslint/eslint-plugin": "21.2.0",
        "@angular-eslint/eslint-plugin-template": "21.2.0",
        "@angular-eslint/schematics": "21.2.0",
        "@angular-eslint/template-parser": "21.2.0",
        "@angular/cli": "21.1.2",
        "@angular/compiler": "21.1.3",
        "@angular/compiler-cli": "21.1.3",
        "@angular/language-service": "21.1.3",
        "@angular/platform-server": "21.1.3",
        "@compodoc/compodoc": "1.2.1",
        "@manniwatch/api-client": "^0.30.0",
        "@manniwatch/client-types": "^0.30.0",
        "@types/jasmine": "6.0.0",
        "@types/jasminewd2": "2.0.13",
        "@types/node": "24.10.12",
        "@typescript-eslint/parser": "8.55.0",
        "angular-eslint": "21.2.0",
        "apply-loader": "2.0.0",
        "codelyzer": "6.0.2",
        "copy-webpack-plugin": "13.0.1",
        "eslint": "9.39.2",
        "express": "5.2.1",
        "jasmine-core": "6.0.1",
        "jasmine-spec-reporter": "7.0.0",
        "karma": "6.4.4",
        "karma-chrome-launcher": "3.2.0",
        "karma-coverage": "2.2.1",
        "karma-coverage-istanbul-reporter": "3.0.3",
        "karma-jasmine": "5.1.0",
        "karma-jasmine-html-reporter": "2.2.0",
        "ng-packagr": "21.1.0",
        "protractor": "7.0.0",
        "tslib": "2.8.1",
        "typed-assert": "1.0.9",
        "typescript": "5.9.3",
        "webpack": "5.104.1",
        "webpack-bundle-analyzer": "5.2.0"
    }
}
