{
  "name": "@manniwatch/client-ng",
  "description": "Angular Client for Manniwatch",
  "version": "0.18.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:pug && ng build",
    "build:pug": "pug ./src --doctype html",
    "build:readme": "npx @appnest/readme generate --input ../package_readme_blueprint.md --config readme_config.json",
    "test": "ng test --watch=false --browsers=ChromeHeadless",
    "test:coverage": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "test:watch": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "postinstall": "npm run build:pug",
    "docs": "npx compodoc -p tsconfig.app.json -d ./docs",
    "prepare": "node ./scripts/prepare.js",
    "prepublishOnly": "npm run test && npm run docs && npm run build",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/manniwatch/stats.json",
    "version": "npm run build:readme && git add README.md"
  },
  "private": false,
  "homepage": "https://manniwatch.github.io/manniwatch/",
  "author": {
    "name": "donmahallem",
    "email": "donmahallem@users.noreply.github.com",
    "url": "https://github.com/donmahallem"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/manniwatch/manniwatch/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manniwatch/manniwatch.git"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/manniwatch/*",
    "src/*"
  ],
  "dependencies": {
    "@angular/animations": "~11.0.9",
    "@angular/cdk": "~11.0.3",
    "@angular/common": "~11.0.9",
    "@angular/compiler": "~11.0.9",
    "@angular/core": "~11.0.9",
    "@angular/forms": "~11.0.9",
    "@angular/localize": "~11.0.9",
    "@angular/material": "~11.0.3",
    "@angular/platform-browser": "~11.0.9",
    "@angular/platform-browser-dynamic": "~11.0.9",
    "@angular/router": "~11.0.9",
    "@angular/service-worker": "~11.0.9",
    "@manniwatch/api-types": "^0.18.0",
    "@manniwatch/assets": "^0.18.2",
    "date-fns": "~2.16.1",
    "material-design-icons": "~3.0.1",
    "ol": "~6.5.0",
    "ol-mapbox-style": "~6.3.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.1.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.6",
    "@angular/cli": "~11.0.6",
    "@angular/compiler-cli": "~11.0.9",
    "@angular/language-service": "~11.0.9",
    "@compodoc/compodoc": "~1.1.11",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~14.14.20",
    "@types/ol": "~6.4.2",
    "apply-loader": "~2.0.0",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "~11.0.3",
    "protractor": "~7.0.0",
    "pug": "~3.0.0",
    "pug-cli": "~1.0.0-alpha6",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~4.0.5",
    "webpack-bundle-analyzer": "~4.3.0"
  }
}
