{
  "name": "@manniwatch/client-ng",
  "description": "Angular Client for Manniwatch",
  "version": "0.29.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "npm run build:pug",
    "build": "ng build --configuration=olvector && ng build",
    "prebuild:build:prod": "npm run build:pug",
    "build:prod": "ng build --aot --configuration=production && ng build --aot --configuration=production,olvector",
    "build:pug": "pug ./src --doctype html --pretty",
    "build:readme": "npx @appnest/readme generate --input ../package_readme_blueprint.md --config readme_config.json",
    "build:release": "ng build --prod --aot",
    "test": "ng test --watch=false --browsers=ChromeHeadless",
    "test:coverage": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "test:watch": "ng test",
    "lint": "ng lint",
    "postlint": "prettier --config ./../../.prettierrc src/**/*.ts src/**/*.scss --check",
    "lint:fix": "ng lint --fix",
    "postlint:fix": "prettier --config ./../../.prettierrc src/**/*.ts src/**/*.scss --write",
    "e2e": "ng e2e",
    "postinstall": "npm run build:pug",
    "docs": "npx compodoc ./src --config ./.compodocrc.json",
    "prepare": "node ./scripts/prepare.js",
    "prepublishOnly": "npm run test && npm run docs && npm run build:prod",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/manniwatch/stats.json",
    "version": "npm run build:readme && git add README.md"
  },
  "browser": {
    "fs": false
  },
  "private": false,
  "homepage": "https://manniwatch.github.io/manniwatch/",
  "author": {
    "name": "donmahallem",
    "email": "donmahallem@users.noreply.github.com",
    "url": "https://github.com/donmahallem"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/manniwatch/manniwatch/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manniwatch/manniwatch.git"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/manniwatch/*",
    "src/*"
  ],
  "dependencies": {
    "@angular/animations": "13.1.2",
    "@angular/cdk": "13.1.2",
    "@angular/common": "13.1.2",
    "@angular/core": "13.1.2",
    "@angular/forms": "13.1.2",
    "@angular/localize": "13.1.2",
    "@angular/material": "13.1.2",
    "@angular/platform-browser": "13.1.2",
    "@angular/platform-browser-dynamic": "13.1.2",
    "@angular/router": "13.1.2",
    "@angular/service-worker": "13.1.2",
    "@donmahallem/rxjs-zone": "0.3.6",
    "@manniwatch/api-types": "^0.29.0",
    "@manniwatch/assets": "^0.29.1",
    "date-fns": "2.28.0",
    "deepmerge": "4.2.2",
    "material-design-icons": "3.0.1",
    "ol": "6.9.0",
    "ol-mapbox-style": "6.8.2",
    "rxjs": "7.5.2",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.1.3",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "13.1.3",
    "@angular/compiler": "13.1.2",
    "@angular/compiler-cli": "13.1.2",
    "@angular/language-service": "13.1.2",
    "@compodoc/compodoc": "1.1.18",
    "@manniwatch/api-client": "^0.29.1",
    "@manniwatch/client-types": "^0.29.1",
    "@types/jasmine": "3.10.3",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "16.11.19",
    "apply-loader": "2.0.0",
    "codelyzer": "6.0.2",
    "eslint": "8.7.0",
    "jasmine-core": "4.0.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.11",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "ng-packagr": "13.1.3",
    "protractor": "7.0.0",
    "pug": "3.0.2",
    "pug-cli": "1.0.0-alpha6",
    "tslib": "2.3.1",
    "typescript": "4.5.4",
    "webpack-bundle-analyzer": "4.5.0"
  }
}
