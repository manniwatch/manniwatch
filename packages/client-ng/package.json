{
  "name": "@manniwatch/client-ng",
  "description": "Angular Client for Manniwatch",
  "version": "0.28.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "npm run build:pug",
    "build": "ng build --configuration=olvector && ng build",
    "prebuild:build:prod": "npm run build:pug",
    "build:prod": "ng build --aot --configuration=production && ng build --aot --configuration=production,olvector",
    "build:pug": "pug ./src --doctype html",
    "build:readme": "npx @appnest/readme generate --input ../package_readme_blueprint.md --config readme_config.json",
    "build:release": "ng build --prod --aot",
    "test": "ng test --watch=false --browsers=ChromeHeadless",
    "test:coverage": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "test:watch": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "postinstall": "npm run build:pug",
    "docs": "npx compodoc ./src --config ./.compodocrc.json",
    "prepare": "node ./scripts/prepare.js",
    "prepublishOnly": "npm run test && npm run docs && npm run build:prod",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/manniwatch/stats.json",
    "version": "npm run build:readme && git add README.md"
  },
  "browser": {
    "fs": false
  },
  "private": false,
  "homepage": "https://manniwatch.github.io/manniwatch/",
  "author": {
    "name": "donmahallem",
    "email": "donmahallem@users.noreply.github.com",
    "url": "https://github.com/donmahallem"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/manniwatch/manniwatch/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manniwatch/manniwatch.git"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/manniwatch/*",
    "src/*"
  ],
  "dependencies": {
    "@angular/animations": "12.2.7",
    "@angular/cdk": "12.2.7",
    "@angular/common": "12.2.7",
    "@angular/core": "12.2.7",
    "@angular/forms": "12.2.7",
    "@angular/localize": "12.2.7",
    "@angular/material": "12.2.7",
    "@angular/platform-browser": "12.2.7",
    "@angular/platform-browser-dynamic": "12.2.7",
    "@angular/router": "12.2.7",
    "@angular/service-worker": "12.2.7",
    "@donmahallem/rxjs-zone": "0.2.6",
    "@manniwatch/api-types": "^0.28.0",
    "@manniwatch/assets": "^0.28.0",
    "date-fns": "2.24.0",
    "deepmerge": "4.2.2",
    "material-design-icons": "3.0.1",
    "ol": "6.8.0",
    "ol-mapbox-style": "6.5.1",
    "rxjs": "7.3.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.7",
    "@angular/cli": "12.2.7",
    "@angular/compiler": "12.2.7",
    "@angular/compiler-cli": "12.2.7",
    "@angular/language-service": "12.2.7",
    "@compodoc/compodoc": "1.1.15",
    "@manniwatch/client-types": "^0.28.0",
    "@types/jasmine": "3.9.1",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "14.17.18",
    "apply-loader": "2.0.0",
    "codelyzer": "6.0.2",
    "jasmine-core": "3.9.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "ng-packagr": "12.2.2",
    "protractor": "7.0.0",
    "pug": "3.0.2",
    "pug-cli": "1.0.0-alpha6",
    "tslib": "2.3.1",
    "webpack-bundle-analyzer": "4.4.2"
  }
}
