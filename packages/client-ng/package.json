{
  "name": "@manniwatch/client-ng",
  "version": "0.1.0",
  "description": "Angular Client for Manniwatch",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "keywords": [
    "api",
    "types"
  ],
  "scripts": {
    "build": "ng build",
    "build:watch": "ng build --watch",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "postinstall": "node ./ng-add-pug-loader.js",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "docs": "npx compodoc -p src/tsconfig.app.json -d ./docs",
    "test:coverage": "nyc --nycrc-path ../../.nycrc.json npm run test"
  },
  "homepage": "https://manniwatch.github.io/manniwatch/",
  "author": {
    "name": "manniwatch",
    "email": "manniwatch@users.noreply.github.com",
    "url": "https://github.com/manniwatch"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/manniwatch/manniwatch/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manniwatch/manniwatch.git"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "dist/*",
    "src/*",
    "!src/**/*.spec.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@angular/animations": "~8.2.0",
    "@angular/cdk": "~8.2.0",
    "@angular/common": "~8.2.0",
    "@angular/compiler": "~8.2.0",
    "@angular/core": "~8.2.0",
    "@angular/forms": "~8.2.0",
    "@angular/material": "~8.2.0",
    "@angular/platform-browser": "~8.2.0",
    "@angular/platform-browser-dynamic": "~8.2.0",
    "@angular/pwa": "~0.803.25",
    "@angular/router": "~8.2.0",
    "@angular/service-worker": "~8.2.0",
    "@donmahallem/trapeze-api-types": "~2.0.1",
    "@types/leaflet": "~1.5.10",
    "core-js": "~3.6.4",
    "leaflet": "~1.6.0",
    "leaflet-rotatedmarker": "~0.2.0",
    "material-design-icons": "~3.0.1",
    "moment": "~2.24.0",
    "ng-cli-pug-loader": "~0.2.2",
    "rxjs": "~6.5.4",
    "tslib": "~1.11.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/cli": "~8.3.25",
    "@angular/compiler-cli": "~8.2.0",
    "@angular/language-service": "~8.2.14",
    "@compodoc/compodoc": "~1.1.11",
    "@types/jasmine": "~3.5.6",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~13.9.0",
    "apply-loader": "~2.0.0",
    "codelyzer": "~5.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "~1.5.2",
    "ngx-i18nsupport": "~0.17.1",
    "protractor": "~6.0.0",
    "pug": "~2.0.4",
    "pug-loader": "~2.4.0",
    "ts-node": "~8.6.2",
    "tslint": "~5.20.1",
    "typescript": ">=3.4.0 <3.6.0"
  }
}