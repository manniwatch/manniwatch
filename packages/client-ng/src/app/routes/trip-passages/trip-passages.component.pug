ng-container(*ngIf="passageService.statusObservable | async as status")
    app-header-box(title="{{status?.tripInfo.routeName}} - {{status?.tripInfo.directionText}}")
        map(appOlStatic,[mapData]="headerMapData")
    div.not-found(*ngIf="status.status==STATUS_OPS.NOT_FOUND")
        h4 not found
    app-trip-passages-list([tripInfo]="status.tripInfo")
