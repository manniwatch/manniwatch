{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ESNext",
        "esModuleInterop": true,
        "sourceMap": true,
        "declaration": true,
        "moduleResolution": "Bundler",
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "skipLibCheck": false,
        "types": []
    },
    "typeAcquisition": {
        "enable": true
    },
    "exclude": [
        "node_modules"
    ],
    "baseUrl": "./src"
}
